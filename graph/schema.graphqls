# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type App {
  id: ID!
  title: String!
  description: String!
  link: String!
  urlType: String!
}

type Query {
  apps: [App!]!
}

input NewApp {
  title: String!
  description: String!
  link: String!
  urlType: String!
}

type Mutation {
  createApp(input: NewApp!): App!
}
